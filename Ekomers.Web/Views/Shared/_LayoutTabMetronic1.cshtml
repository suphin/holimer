	<script>
		$(document).ready(function () {
			$(".open-tab").click(function (e) {
				e.preventDefault();

				var url = $(this).data("url");
				var tabId = url.replace(/\//g, "_"); // URL'yi ID'ye çevirme

				// Eğer sekme zaten açıksa doğrudan ona geç
				if ($("#tab-" + tabId).length > 0) {
					$("#tab-" + tabId).tab("show");
					return;
				}

				// Yeni sekme ekle
				$("#tabMenu").append(
					'<li class="nav-item">' +
						'<a class="nav-link active" id="tab-' + tabId + '" data-bs-toggle="tab" href="#content-' + tabId + '">' +
							$(this).text() +
							' <span class="close-tab" data-id="' + tabId + '" style="cursor:pointer; margin-left:5px;"> ×</span>' +
						'</a>' +
					'</li>'
				);

				// Yeni içerik ekle
				$("#tabContent").append('<div class="tab-pane fade show active" id="content-' + tabId + '">Yükleniyor...</div>');

				// AJAX ile içeriği yükle
				$.get(url, function (data) {
					$("#content-" + tabId).html(data);
				});

				// Diğer sekmeleri pasif hale getir
				$(".nav-link").removeClass("active");
				$(".tab-pane").removeClass("show active");

				// Yeni sekmeyi aktif yap
				$("#tab-" + tabId).addClass("active");
				$("#content-" + tabId).addClass("show active");
			});

			// Sekme kapatma fonksiyonu
			$(document).on("click", ".close-tab", function () {
				var tabId = $(this).data("id");
				$("#tab-" + tabId).parent().remove();
				$("#content-" + tabId).remove();

				// Eğer kapatılan sekme aktifse, son sekmeye geç
				if ($(".nav-tabs .nav-link").length > 0) {
					$(".nav-tabs .nav-link").last().tab("show");
				}
			});
		});
	</script>
